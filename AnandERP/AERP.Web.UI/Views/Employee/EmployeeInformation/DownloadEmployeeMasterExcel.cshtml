@model AERP.ViewModel.EmpEmployeeMasterViewModel
<div class="mfp-content" style="width: 100%;">
    <div id="custom-content">
        <div class="block-header">
            <h2>&nbsp;<i class="zmdi zmdi-view-toc"></i>&nbsp;Employee Attendence</h2>
        </div>
        <div class="card">
            <div class="card-body card-padding">
                <div id="displayErrorMessagee" role="alert" hidden class="alert">
                    <button class="close" type="button"><span aria-hidden="true">×</span></button>
                    <p id="messageContent"></p>
                </div>
                @using (Html.BeginForm("DownloadEmployeeExcel", "EmployeeInformationExcelUpload", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                    <form id="FormCreateTotalEmployeeMasterExcel" method="post">
                        <br /><br />

                        <div class="form-group">
                            <div class="row">
                                <div class="col-sm-6">
                                    @Html.HiddenFor(m => m.CentreCode)
                                   @Html.LabelFor(model => model.Details, new { style = "" })
                                    <div class="form-group  fg-line select">
                                        @Html.DropDownList("Details", ViewData["Details"] as List<SelectListItem>, new { style = "", @class = "form-control" })
                                    </div>
                                    @Html.ValidationMessageFor(model => model.Details)
                                </div>
                            </div>
                            

                        </div>
                        <div class="form-group">
                                <input class="btn btn-primary btn-sm m-t-10 waves-effect" value="@Resources.ButtonAction_Submit" style="margin-left:3%;" id="createemployeerecordexcel" type="submit"  />
                            </div>

                    </form>
                }
            </div>
        </div>
    </div>
    <script src="@Url.Content("~/Scripts/jquery.validate.js") " type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            EmployeeInformation.Initialize();
        });
    </script>
    <script src="@Url.Content("~/Content/Theme/vendors/bower_components/moment/min/moment.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Theme/vendors/bower_components/eonasdan-bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Theme/vendors/fileinput/fileinput.min.js")" type="text/javascript"></script>
</div>




