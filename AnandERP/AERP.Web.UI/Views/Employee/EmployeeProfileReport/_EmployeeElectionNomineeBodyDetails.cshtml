@model AMS.ViewModel.EmployeeElectionNomineeBodyViewModel
@{

    Layout = null;
}
@Html.HiddenFor(model => model.EmployeeID)

@if (ViewBag.Data == 1)
{ 

    <div class="col-sm-12">
            <div class="card rating-list">
                <div class="listview">
                    <div class="lv-header">
                        <div class="m-t-0">
                            @Resources.DisplayName_MembershipofLearnedSociety
                        </div>
                        <div class="clearfix"></div>
                    </div>

                    @*<div class="lv-body">*@
                        <div class="table-responsive" id="DataTables_Table_0_wrapper">
                            <table id="DataTables_Table_0" class="table">
                                <thead>
                                    <tr class="warning c-white">
                                        <th>@Resources.TableHeaders_Nameofthesociety</th>
                                        <th>@Resources.TableHeaders_PostHeld</th>
                                        <th>@Resources.TableHeaders_Duration</th>
                                    </tr>
                                </thead>
                                @foreach (var item in ViewBag.ListEmployeeElectionNomineeBodyDetails)
                                {
                                    <tbody>
                                        <tr class="active">
                                            <td>@item.NameOfBoardBody</td>
                                            <td>@item.PostHeld</td>
                                            <td>@item.FromDate - @item.ToDate</td>
                                        </tr>
                                    </tbody>
                                }
                            </table>
                        </div>
                    @*</div>*@
                </div>
            </div>
        </div>
}