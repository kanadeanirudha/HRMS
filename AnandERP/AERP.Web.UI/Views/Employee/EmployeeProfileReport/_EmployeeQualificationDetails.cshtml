@model AMS.ViewModel.EmployeeQualificationViewModel
@{

    Layout = null;
}
@Html.HiddenFor(model => model.EmployeeID)

@if (ViewBag.Data == 1)
{ 
    <div class="col-sm-12">
            <div class="card rating-list">
                <div class="listview">
                    <div class="lv-header">
                        <div class="m-t-0">
                            @Resources.DisplayName_EducationalQualification
                        </div>
                        <div class="clearfix"></div>
                    </div>

                    @*<div class="lv-body">*@
                        <div class="table-responsive" id="DataTables_Table_0_wrapper">
                            <table id="DataTables_Table_0" class="table">
                                <thead>
                                    <tr class="warning c-white">
                                        <th>@Resources.TableHeaders_DegreeName </th>
                                        <th>@Resources.TableHeaders_CollegeInstitute </th>
                                        <th>@Resources.TableHeaders_Univarsity </th>
                                        <th>@Resources.TableHeaders_Yearofpassingawarded </th>
                                    </tr>
                                </thead>
                                @foreach (var item in ViewBag.ListEmployeeQualificationDetails)
                                {
                                    <tbody>
                                        <tr class="active">
                                            <td>@item.EducationName</td>
                                            <td>@item.NameOfInstitution </td>
                                            <td>@item.UniversityName</td>
                                            <td>@item.YearOfPassing</td>
                                        </tr>
                                    </tbody>
                                }
                            </table>
                        </div>
                    @*</div>*@
                </div>
            </div>
        </div>

}