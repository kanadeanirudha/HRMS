@model AERP.ViewModel.AccountTransactionTypeMasterViewModel
<div id="custom-content">
    <div class="block-header">
        <h2>&nbsp;<i class="zmdi zmdi-view-toc"></i>&nbsp; &nbsp;Create Account Transaction Type</h2>
    </div>
    <div class="card">
        <div class="card-body card-padding">
            <div id="displayErrorMessage" role="alert" hidden class="alert">
                <button class="close" type="button"><span aria-hidden="true">×</span></button>
                <p id="messageContent"></p>
            </div>
            <form id="FormCreateAccountTransactionTypeMaster" role="form" class="row">
            <div class="row">
                <div class="form-group col-sm-6">
                    <div class="form-group fg-line">
                        @Html.LabelFor(model => model.TransactionTypeName, new { })
                        @Html.TextBoxFor(model => model.TransactionTypeName, new { maxlength = "20", @class = "form-control input-sm " })

                    </div>
                    @Html.ValidationMessageFor(model => model.TransactionTypeName)
               </div>

                <div class="form-group col-sm-6">
                    <div class="form-group fg-line">
                        @Html.LabelFor(model => model.TransactionTypeCode, new { })
                        @Html.TextBoxFor(model => model.TransactionTypeCode, new { maxlength = "2", @class = "form-control input-sm " })

                    </div>
                    @Html.ValidationMessageFor(model => model.TransactionTypeCode)
                    </div>
             </div>
                <div class="form-group">
                    <input class="btn btn-primary btn-sm m-t-10 waves-effect" value="@Resources.ButtonAction_Submit" id="CreateAccountTransactionTypeMasterRecord" type="button" />
                </div>

            </form>
        </div>
    </div>
</div>

<script src="@Url.Content("~/Scripts/jquery.validate.js") " type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        AccountTransactionTypeMaster.Initialize();
    });
</script>

