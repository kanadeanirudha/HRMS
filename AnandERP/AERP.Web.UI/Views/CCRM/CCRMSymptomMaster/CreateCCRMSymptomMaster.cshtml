@model AERP.ViewModel.CCRMSymptomMasterViewModel
<script src="@Url.Content("~/Content/Theme/vendors/bower_components/moment/min/moment.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/Theme/vendors/bower_components/eonasdan-bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js")" type="text/javascript"></script>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js") " type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<style>
    .mfp-close-btn-in .mfp-close {
        color: #eee;
    }
</style>
<div class="mfp-content" style="background :#bfc6ca;width: 902px;margin: -10px 0px -10px -10px;">
    <div id="custom-content">
        <div class="block-header">
            <h2 style="background :#607d8b;color:white;margin-top: -10px;line-height: 2.5;width: 902px;margin-left: -10px;">&nbsp;<i class="zmdi zmdi-view-toc"></i>&nbsp; &nbsp;Symptom Master </h2>
        </div>
        <div class="card">
            <div class="card-body card-padding" style="background:white;margin-top:-25px;margin-left: -10px;width: 902px;">
                <div id="displayErrorMessage" role="alert" hidden class="alert">
                    <button class="close" type="button"><span aria-hidden="true">×</span></button>
                    <p id="messageContent"></p>
                </div>
                <form id="FormCreateCCRMSymptomMaster" role="form" class="row">
                    <div class="row">
                        <div class="form-group col-sm-6">
                            <div class="form-group fg-line">
                                @Html.HiddenFor(m => m.ID)
                                @Html.LabelFor(model => model.SymptomTypeTitle, new { })
                                @Html.TextBoxFor(model => model.SymptomTypeTitle, new { @class = "form-control input-sm ", disabled = "disabled" })

                            </div>
                            @Html.ValidationMessageFor(model => model.SymptomTypeTitle)
                        </div>
                        <div class="form-group col-sm-6">
                            <div class="form-group fg-line">
                                @Html.HiddenFor(m => m.ID)
                                @Html.LabelFor(model => model.SymptomTypeCode, new { })
                                @Html.TextBoxFor(model => model.SymptomTypeCode, new { @class = "form-control input-sm ", disabled = "disabled" })

                            </div>
                            @Html.ValidationMessageFor(model => model.SymptomTypeCode)
                        </div>

                        <div class="form-group col-sm-6">
                            <div class="form-group fg-line">

                                @Html.LabelFor(model => model.SymptomTitle, new { })
                                @Html.TextBoxFor(model => model.SymptomTitle, new { @class = "form-control input-sm ", })

                            </div>
                            @Html.ValidationMessageFor(model => model.SymptomTitle)
                        </div>
                        <div class="form-group col-sm-6">
                            <div class="form-group fg-line">

                                @Html.LabelFor(model => model.SymptomCode, new { })
                                @Html.TextBoxFor(model => model.SymptomCode, new { @class = "form-control input-sm ", })

                            </div>
                            @Html.ValidationMessageFor(model => model.SymptomCode)
                        </div>
                        <div class="form-group col-sm-6">
                            <div class="form-group fg-line">

                                @Html.LabelFor(model => model.SymptomDescription, new { })
                                @Html.TextBoxFor(model => model.SymptomDescription, new { @class = "form-control input-sm ", })

                            </div>
                            @Html.ValidationMessageFor(model => model.SymptomDescription)
                        </div>

                    </div>

                    <div class="form-group col-sm-6">
                        <input class="btn btn-info" style="margin-left:1%" value="@Resources.ButtonAction_Submit" id="CreateCCRMSymptomMasterRecord" type="button" />
                    </div>


                </form>
            </div>
        </div>
    </div>

    <script src="@Url.Content("~/Scripts/jquery.validate.js") " type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            CCRMSymptomMaster.Initialize();
        });
    </script>
</div>



