                                    {
                                        if (item.SaleContractEmployeeMasterID == Convert.ToInt32(EmployeeIDList[0]) && item.SaleContractManPowerItemID == ManPowerItemID && item.RuleType == "Allowance" && item.HeadType == "RIA")
                                        {
                                            <th>@item.HeadName</th>
                                            <th>Total Payable Amount</th>
                                        }
                                    }


