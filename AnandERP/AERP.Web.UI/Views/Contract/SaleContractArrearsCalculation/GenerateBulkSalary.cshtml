                                        if (item.SaleContractEmployeeMasterID == Convert.ToInt32(EmployeeIDList[0]) && item.SaleContractManPowerItemID == ManPowerItemID && item.RuleType == "Deduction" && item.ContributionType == 1 && item.ComplianceType == 2)
                                        {
                                            <th style="">@item.HeadName</th>
                                        }
                                    }
                                    <th>Total Salary</th>
                                    @foreach (var item in Model.SaleContractArrearsCalculationList)
                                    {
                                        if (item.SaleContractEmployeeMasterID == Convert.ToInt32(EmployeeIDList[0]) && item.SaleContractManPowerItemID == ManPowerItemID && item.RuleType == "Deduction" && item.ContributionType == 2)
                                        {
                                            <th>@item.HeadName</th>
                                        }
                                    }
                                    <th>Employer Contribution</th>
                                    <th style="">Total CTC</th>
                                </tr>
                            </thead>

                            <tbody style="overflow-y:auto;overflow-x:hidden;height:200px;display:block;">
                                @if (EmployeeIDList.Count() > 0)
