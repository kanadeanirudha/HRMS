                                    {
                                        if (item.SaleContractEmployeeMasterID == Convert.ToInt32(EmployeeIDList[0]) && item.SaleContractManPowerItemID == ManPowerItemID && item.RuleType == "Deduction" && item.ContributionType == 1 && item.HeadType == "PT" && PTAdded == false)
