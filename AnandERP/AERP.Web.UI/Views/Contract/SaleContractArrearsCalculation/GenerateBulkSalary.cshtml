                                    for (int i = 0, j = 0, isIncremented = 0; i < EmployeeIDList.Count(); i++)
                                    {
                                        bool PTValueAdded = false;
                                        decimal BasicAmount = Math.Round(Model.SaleContractArrearsCalculationList[j].TotalAmount, MidpointRounding.AwayFromZero);
                                        decimal PreviousBasicAmount = Math.Round(Model.SaleContractArrearsCalculationList[j].PreviousTotalAmount, MidpointRounding.AwayFromZero);
                                        decimal TotalAttendance = Model.SaleContractArrearsCalculationList[j].TotalAttendance;
                                        byte TotalDays = Model.SaleContractArrearsCalculationList[j].TotalDays;
                                        int ManPowerId = Model.SaleContractArrearsCalculationList[j].SaleContractManPowerItemID;

                                        FinalTotalDays += TotalAttendance;

                                        decimal ActualBasicAmount = Math.Round((BasicAmount / TotalDays) * TotalAttendance, MidpointRounding.AwayFromZero);
                                        decimal PreviousActualBasicAmount = Math.Round((PreviousBasicAmount / TotalDays) * TotalAttendance, MidpointRounding.AwayFromZero);

                                        decimal MainBasicAmount = ActualBasicAmount - PreviousActualBasicAmount;

                                        FinalTotalBasic += MainBasicAmount;

                                        FinalOTDays += Model.SaleContractArrearsCalculationList[j].OvertimeHours;

                                        decimal FixedSalaryAmount = Math.Round(Model.SaleContractArrearsCalculationList[j].FixedSalaryAmount, MidpointRounding.AwayFromZero);
                                        decimal ActualFixedSalaryAmount = Math.Round((FixedSalaryAmount / TotalDays) * TotalAttendance, MidpointRounding.AwayFromZero);

                                        decimal TotalAmount = MainBasicAmount;
                                        decimal ActualTotalAmount = ActualBasicAmount - PreviousActualBasicAmount;
                                        // decimal PreviousTotalAmount = PreviousBasicAmount;
                                        decimal PreviousActualTotalAmount = PreviousActualBasicAmount;

                                        decimal GrossSalary = MainBasicAmount;
                                        decimal ActualGrossSalary = MainBasicAmount;
                                        decimal ActualTotalEarnings = MainBasicAmount;
                                        decimal TotalDeduction = 0;
                                        decimal ActualTotalDeduction = 0;
                                        decimal EmployerContribution = 0;
                                        decimal ActualEmployerContribution = 0;
                                        decimal NetPayable = 0; decimal ActualNetPayable = 0; decimal TotalSalary = 0; decimal ActualTotalSalary = 0; decimal ActualSalaryWithAdditionalAllowance = 0; decimal SalaryWithAdditionalAllowance = 0;
                                        bool AdditionalInTotal = false; bool FixedAmountAdded = false;

