@model AERP.ViewModel.SaleContractJobWorkDataViewModel

<table id="myDataTable" class="table table-striped table-vmiddle table-responsive ">
    <thead>
        <tr>
            <th>Job Work Item Name</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.SaleContractJobWorkDataList)
        {
            <tr>
                <td>@item.SaleContractJobWorkItemName</td>
                <td>@item.Quantity</td>
            </tr>
        }
    </tbody>
</table>
