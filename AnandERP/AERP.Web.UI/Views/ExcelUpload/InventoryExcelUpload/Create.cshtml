@model AERP.ViewModel.GeneralEducationTypeMasterViewModel

<form id="FormCreateGeneralEducationTypeMaster" method="post">
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js") " type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <div id="update-message" class="error invisible"></div>

    <div class="widget-block">
        <div class="widget-head">
            <h5>@Resources.FormCreate_EducationType </h5>
        </div>
        <div class="widget-content">
            <div class="widget-box" style="padding: 15px">

                        <div class="control-group control-label">
                       
  <span style="color: red; float: left; padding-left: 1px; font-weight: bold">*</span>      @Html.LabelFor(model => model.Description, new { style = "font-Weight:Bold;" })                        						
                            <div class="controls input-xlarge text-tip">
                                @Html.TextBoxFor(model => model.Description, new { maxlength = "30", style = "font-Weight:Bold" })
                                <br />
                                @Html.ValidationMessageFor(model => model.Description)
                              
                            </div>
                        </div>
                       @* <div class="control-group control-label">
                            @Html.LabelFor(model => model.EduSequenceNumber, new { style = "font-Weight:Bold;" })                       						
                            <div class="controls input-xlarge text-tip">
                                @Html.TextBoxFor(model => model.EduSequenceNumber, new { min = "0", type = "number" , Value = "" })
                                <br />
                                @Html.ValidationMessageFor(model => model.EduSequenceNumber)
                            </div>
                        </div>*@

                       
                        <div class="form-actions" style="border: 1px solid #e5e5e5;text-align:right">

                             
                            <input id="CreateGeneralEducationTypeMasterRecord" type="button" value="@Resources.ButtonAction_Submit" class="btn btn-primary">
                            <input name="" id="reset" type="submit" class="btn btn-secondary" value="@Resources.ButtonAction_Reset" />
                        </div>
                  
            </div>
        </div>
    </div>
</form>
<script type="text/javascript">
    $(document).ready(function () {
        GeneralEducationTypeMaster.Initialize();
    });
</script>
