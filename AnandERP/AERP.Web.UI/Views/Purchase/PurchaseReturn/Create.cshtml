    @model AERP.ViewModel.PurchaseReturnViewModel
       <script src="@Url.Content("~/Content/Theme/vendors/bower_components/moment/min/moment.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Theme/vendors/bower_components/eonasdan-bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js")" type="text/javascript"></script>
      <link href="@Url.Content("~/Content/jquery-ui.theme.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/admin/jquery-ui-1.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery.validate.min.js") " type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>



<div id="custom-content">
 <div class="block-header">
        <h2>&nbsp;<i class="zmdi zmdi-view-toc"></i>&nbsp; &nbsp;Create Purchase Return </h2>
    </div>
 <div class="card">
    <form id="FormCreatePurchaseReturnMaster">
         <div id="displayErrorMessage" role="alert" hidden class="alert">
           <button class="close" type="button"><span aria-hidden="true">×</span></button>
             <p id="SectionErrorMessage"></p>
          </div>
     <div class="card-body card-padding">
                <div class="row">
                   <div class="col-sm-12">
                    <div class="col-sm-4">
                       Vendor
                        @Html.HiddenFor(m => m.CreatedBy)
                         @Html.HiddenFor(m => m.VendorId)
                         @Html.HiddenFor(model => model.VendorNumber)
                         @Html.HiddenFor(model => model.LocationID)
                        <div class="form-group fg-line ">
                              @Html.TextBoxFor(m => m.vendor, new { @class = "form-control input-sm typeahead"})
                        </div>
                    </div>

                    <div class="col-sm-4">
                        Transaction Date
                        <div class="form-group fg-line">
                             @Html.TextBoxFor(model => model.TransactionDate, new { @class = "form-control input-sm " })
                             
                         </div>
                    </div>
                
                        <div class="col-sm-3">
                       Location
                        <div class="form-group fg-line">
                           @Html.DropDownList("GeneralLocationList", ViewData["GeneralLocationList"] as List<SelectListItem>, "---Select Location ---",new { @class = "form-control input-sm" })
                         </div>
                    </div>
                   
                  </div>
                 </div>
            </div>
            <hr />
                         <div class="card-body card-padding">
                             <div class="row">
                                 <div class="col-sm-3">
                                     @Html.HiddenFor(m => m.ItemNumber)
                                     @Html.HiddenFor(m => m.BaseUOMQuantity)
                                     @Html.HiddenFor(m => m.BaseUOMCode)
                                     @Html.HiddenFor(m => m.PurchaseGRNMasterID)
                                     @Html.HiddenFor(model => model.PurchaseOrderMasterID)
                                     @Html.HiddenFor(m => m.GenTaxGroupMasterId)
                                     @Html.HiddenFor(model => model.TaxInPercentage)
                                     @Html.HiddenFor(model => model.GeneralItemCodeID)
                                     @Html.HiddenFor(model => model.ExpiryDate)
                                     @Html.HiddenFor(model => model.BatchID)
                                     @Html.HiddenFor(m => m.SerialAndBatchManagedBy)
                                     @Html.HiddenFor(m => m.TaxRateList)
                                     @Html.HiddenFor(m => m.TaxList)
                                     
                                    
                                     @Html.LabelFor(model => model.ItemDescription, new { Style = "" })
                                     <div class="form-group fg-line">
                                         @Html.TextBoxFor(model => model.ItemDescription, new { @class = "form-control input-sm typeahead" })
                                     </div>
                                 </div>

                                 <div class="col-sm-3">
                                     @Html.LabelFor(model => model.PurchaseOrderNumber, new { Style = "" })
                                     <div class="form-group fg-line">
                                         @Html.TextBoxFor(model => model.PurchaseOrderNumber, new { @class = "form-control input-sm typeahead" })
                                     </div>
                                 </div>

                                 <div class="col-sm-3">
                                     @Html.HiddenFor(model => model.ReceivedQuantity)
                                     @Html.LabelFor(model => model.PurchaseGrnNumber, new { Style = "" })
                                     <div class="form-group fg-line">
                                         @Html.TextBoxFor(model => model.PurchaseGrnNumber, new { @class = "form-control input-sm typeahead" })
                                     </div>
                                 </div>
                               
                                 <div class="col-sm-3">
                                     @Html.LabelFor(model => model.BatchNumber, new { Style = "" })
                                     <div class="form-group fg-line">
                                         @Html.TextBoxFor(model => model.BatchNumber, new { @class = "form-control input-sm typeahead" })
                                     </div>
                                 </div>
                                 
                                 
                             </div>
                                   <div class="row">
                                       <div class="col-sm-3" id="Units">
                                           @Html.LabelFor(model => model.UnitCode, new { Style = "" })
                                           <div class="form-group fg-line">
                                               @* @Html.TextBoxFor(model => model.UnitCode, new {  @class = "form-control", disabled="disabled" ,style="margin-top:-6px;"})*@
                                               @Html.DropDownListFor(model => model.UnitCode, Enumerable.Empty<SelectListItem>(), new { @class = "form-control input-sm", data_val = "true", data_toggle = "tooltip", data_placement = "top", data_original_title = "" })
                                           </div>
                                       </div>

                                       <div class="col-sm-3">
                                           @Html.LabelFor(model => model.Quantity, new { Style = "" })
                                           <div class="form-group fg-line">
                                               @Html.TextBoxFor(model => model.Quantity, new { @class = "form-control", style = "background-color:#fff;margin-top:-6px;" })
                                           </div>
                                       </div>

                                       <div class="col-sm-2">
                                           @Html.LabelFor(model => model.Rate, new { Style = "" })
                                           <div class="form-group fg-line">
                                               @Html.TextBoxFor(model => model.Rate, new { @class = "form-control", disabled = "disabled", style = "margin-top:-6px;" })
                                           </div>
                                       </div>

                                       <div class="col-sm-2 pull-left" style="margin-top:8px;">
                                           <button type="button" class="btn btn-login btn-success btn-float waves-effect waves-circle waves-float" id="addItem" style="height: 40px; width:          40px; line-height: 35px !important;" value="@Resources.ButtonAction_AddRow" title="@Resources.ButtonAction_AddRow"><i class="zmdi zmdi-plus"></i></button>
                                       </div>
                                   </div>
                       
                   <div class="row" id="SectionDetails" >
                    <div class="col-sm-12">
                        
                        <div class="row">
                            <div id="DivAddRowTable">
                                    <table id="tblData" class="table">
                                        <thead>
                                            <tr>
                                                <th>Item Description</th>
                                                <th>PO Number</th>
                                                <th>GRN Number</th>
                                                <th>Unit</th>
                                                 <th>Qty</th>
                                                <th>Rate</th>
                                                <th>Batch Number</th>
                                                <th>Amount</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                           </div>
                        </div>
                        </div>
                              <hr />
                               <br />
                              <div class="row">
                                        <div class="col-sm-12">
                                            <div class="col-sm-2">
                                                Total Tax Amount
                                                <div class="form-group fg-line">
                                                    <input class="form-control" type="text" style="" disabled id="TotalTaxAmount" />
                                                </div>
                                            </div>

                                            <div class="col-sm-2">
                                                  Total Amount
                                                  <div class="form-group fg-line">
                                                        <input class="form-control" type="text" style="" disabled id="TotalBillAmount" />
                                                   </div>
                                               </div>
                                              <div class="col-sm-2">
                                                    Total Gross Amount
                                                    <div class="form-group fg-line">
                                                        <input class="form-control" type="text" style="" disabled id="TotalGrossAmmount" />
                                                    </div>
                                                </div>
                                            <div>
                                                <div class="col-sm-2" style="margin-top:20px;">
                                                    <div class="form-group fg-line">
                                                        <input type="button" id="CreatePurchaseReturn" value="@Resources.ButtonAction_Submit" class="btn btn-primary" />
                                                    </div>
                                                </div>
                                    </div>
                                </div>
                    </div>
                </form>
               </div>
           </div>
     
    <script src="@Url.Content("~/Content/Theme/vendors/bower_components/moment/min/moment.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Theme/vendors/bower_components/eonasdan-bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js")" type="text/javascript"></script>
    <script src="~/Content/Theme/vendors/bower_components/typeahead.js/dist/typeahead.bundle.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            PurchaseReturn.Initialize();
        });
    </script>



                                                      

