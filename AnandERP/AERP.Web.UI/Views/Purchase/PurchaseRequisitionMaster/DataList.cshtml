        $(this).closest('tr').find('td[id=TaxAmountForDisplay]').text((((parseFloat(quantity * rate).toFixed(2)) * TaxRate) / 100).toFixed(2));
        

        Amount = (parseFloat(quantity * rate).toFixed(2));
