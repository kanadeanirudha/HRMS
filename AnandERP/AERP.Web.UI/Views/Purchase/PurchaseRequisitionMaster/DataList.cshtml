    $("#tblDataPRQ tbody").on("keyup", "tr td input[id^=ApprovedQuantity]", function () {
        var selectedItemID = $("select[id=checkboxlist1]").val();

        var quantity = $(this).closest('tr').find('td input[id^=ApprovedQuantity]').val();
